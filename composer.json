                    "reference": "master"
                },
                "autoload": {
                    "psr-0": {"PHPTAL": "classes/"}
                }
            }
        }
    ], 
    "require": {
        "phptal/phptal": "1.2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
    	"psr-0": { "Vizualizer": "src" }
    }
